<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <script>
        let a = parseInt(prompt("nhập số thứ nhất"))
        let b = parseInt(prompt("nhập số thứ hai"))
        let c = parseInt(prompt("nhập số thứ ba"))
        if(a>b && a>c){
            alert(a)
        } else if ( b >a && b > c ){
            alert(b)
        } else if ( c > a && c > b){
            alert(c)
        }
    
        
    </script>
</body>
</html>
