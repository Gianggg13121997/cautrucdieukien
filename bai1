<body>

    <script>
        let a = parseInt(prompt("nhập số thứ nhất"))
        let b = parseInt(prompt("nhập số thứ hai"))
        if (a % b == 0) {
            alert("  có chia hết  ")
        } else if ( a % b != 0) {
            alert("  không chia hết  ")
        } else if (" b % a == 0"){
            alert (" chia hết ")
        } else {
            alert (" không chia hết ")
        }
    </script>
</body>

